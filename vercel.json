{
  "version": 2,
  "name": "agrirs-lab-inpe",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/dashboard/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/posts/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/membros/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/enviar",
      "dest": "/api/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/dashboard/(.*)\\.html$",
      "dest": "/dashboard/$1.html"
    },
    {
      "src": "/dashboard/dashboard_html/(.*)\\.html$",
      "dest": "/dashboard/dashboard_html/$1.html"
    },
    {
      "src": "/dashboard/login\\.html$",
      "dest": "/dashboard/login.html"
    },
    {
      "src": "/dashboard/dashboard_css/(.*)",
      "dest": "/dashboard/dashboard_css/$1"
    },
    {
      "src": "/dashboard/dashboard_js/(.*)",
      "dest": "/dashboard/dashboard_js/$1"
    },
    {
      "src": "/pages/(.*)\\.html$",
      "dest": "/pages/$1.html"
    },
    {
      "src": "/en-pages/(.*)\\.html$",
      "dest": "/en-pages/$1.html"
    },
    {
      "src": "/src/(.*)",
      "dest": "/src/$1"
    },
    {
      "src": "/$",
      "dest": "/pages/home.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}
